{
  "name": "jupyterlab-tutorial-manager",
  "version": "0.5.0",
  "description": "An extension that allows interactive tutorials to be incorporated.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/downiec/jupyterlab-tutorial-manager.git",
  "bugs": {
    "url": "https://github.com/downiec/jupyterlab-tutorial-manager.git/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Carlos Downie",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/downiec/jupyterlab-tutorial-manager.git.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w",
    "tslint-check": "npx tslint-config-prettier-check ./tslint.json",
    "lint": "npx tslint 'src/**/*.{ts,tsx}'",
    "lint-file": "npx tslint ",
    "lint-fix-file": "npx tslint --fix ",
    "format": "npx prettier-tslint fix 'src/**/*.{ts,tsx,css,scss}'"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0-alpha.11",
    "@jupyterlab/mainmenu": "^1.0.0-alpha.11",
    "@types/react-joyride": "^2.0.2",
    "@types/react-dom": "^16.8.4",
    "@types/react": "^16.8.20",
    "react-joyride": "^2.0.5",
    "react-dom": "^16.8.4",
    "react": "^16.8",
    "prop-types": "^15.0.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "prettier": "^1.18.2",
    "prettier-tslint": "^0.4.2",
    "typescript": "^3.5.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0"
  },
  "jupyterlab": {
    "extension": true
  }
}
